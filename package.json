{
  "name": "n8n-railway-deployment",
  "version": "1.0.0",
  "description": "Production-ready n8n self-hosted deployment for Railway",
  "scripts": {
    "start": "n8n",
    "dev": "docker-compose up",
    "down": "docker-compose down",
    "logs": "docker-compose logs -f n8n",
    "cli": "python cli.py",
    "cli:health": "python cli.py health",
    "cli:validate": "python cli.py validate",
    "cli:info": "python cli.py info",
    "test": "python cli.py test --coverage",
    "test:unit": "python cli.py test --unit",
    "test:integration": "python cli.py test --integration",
    "lint": "python cli.py lint",
    "lint:fix": "python cli.py lint --fix",
    "format": "python cli.py format",
    "check": "python cli.py lint && python cli.py test --coverage"
  },
  "keywords": [
    "n8n",
    "workflow",
    "automation",
    "railway",
    "deployment"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT"
}
